import QtQuick 2.5
import QtQuick.Controls 1.4

Text {
    id: text_label
	font.family: "Arial"
    font.bold: true
    font.pixelSize: 13
	font.weight: Font.Bold
	horizontalAlignment: Text.AlignRight
	verticalAlignment: Text.AlignVCenter
    color: "#99ff99"
	styleColor: "#000000"
	style: Text.Outline
}
